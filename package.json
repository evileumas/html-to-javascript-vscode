{
    "name": "html-to-javascript-string",
    "displayName": "html to javascript string",
    "description": "Converts html code to javascript string",
    "version": "0.0.2",
    "publisher": "evileumas",
    "repository": {
        "type": "git",
        "url": "https://github.com/evileumas/html-to-javascript-vscode.git"
    },
    "icon": "./icon/html-to-javascript-vscode.png",
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "html",
        "conversor",
        "javascript",
        "string",
        "converter"
    ],
    "activationEvents": [
        "onCommand:extension.convertHTMLtoString"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.convertHTMLtoString",
                "title": "Convert HTML to Javascript String"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "when": "editorHasSelection",
                    "command": "extension.convertHTMLtoString",
                    "group": "modification"
                }
            ]
        },
        "keybindings": [
            {
                "when": "editorHasSelection",
                "command": "extension.convertHTMLtoString",
                "group": "modification",
                "key": "ctrl+h ctrl+j"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}